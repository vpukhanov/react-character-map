{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "jsx": "react",
        "outDir": "./dist",
        "strict": true,
        "downlevelIteration": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true
    },
    "include": [
        "src"
    ]
}
